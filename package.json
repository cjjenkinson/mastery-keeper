{
  "name": "masterykeeper",
  "version": "1.0.0",
  "description": "Generate insights from your RescueTime daily summaries",
  "main": "build/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/main.js",
    "dev": "cross-env NODE_ENV=development cross-env DEBUG=keeper* backpack",
    "test": "node_modules/.bin/ava tests/**/*.test.js --verbose",
    "test:watch": "node_modules/.bin/ava --verbose --watch",
    "coverage": "node_modules/.bin/nyc yarn test",
    "check-coverage": "node_modules/.bin/nyc check-coverage --lines 100 --functions 100 --branches 100 --statements 100",
    "prebuild": "rimraf build",
    "build": "backpack build",
    "postbuild": "cp package.json build/package.json && cp now.json build/now.json",
    "deploy": "now",
    "lint": "eslint ."
  },
  "author": "Cameron Jenkinson",
  "license": "MIT",
  "dependencies": {
    "ava": "^0.25.0",
    "axios": "^0.18.0",
    "babel-register": "^6.26.0",
    "backpack-core": "^0.7.0",
    "cron": "^1.3.0",
    "cross-env": "^5.1.6",
    "joi": "^13.4.0",
    "koa": "^2.5.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "lowdb": "^1.0.0",
    "nodemailer": "^4.6.7",
    "now-env": "^3.1.0",
    "nyc": "^12.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "debug": "^3.1.0",
    "eslint": "^5.0.1",
    "eslint-config-strongloop": "^2.1.0",
    "prettier": "^1.13.7"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  }
}
